---
<%= metadata.name %>::package_name: '<%= metadata.name %>'
<%= metadata.name %>::service_name: '<%= metadata.name %>'
<%= metadata.name %>::config_file_path:  '/etc/<%= metadata.name %>/<%= metadata.name %>.conf'
<%= metadata.name %>::config_file_mode: '0644'
<%= metadata.name %>::config_file_owner: 'root'
<%= metadata.name %>::config_file_group: 'root'
<%= metadata.name %>::config_dir_path: '/etc/<%= metadata.name %>'
<%= metadata.name %>::init_file_path:  '/etc/sysconfig/<%= metadata.name %>'
<%= metadata.name %>::process_user: '<%= metadata.name %>'
